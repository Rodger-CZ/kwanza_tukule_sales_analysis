   "source": [
    "ts[\"z_score\"] = (\n",
    "    (ts[\"unit_price\"] - ts[\"unit_price\"].mean()) / ts[\"unit_price\"].std()\n",
    ")\n",
    "\n",
    "anomalies = ts[ts[\"z_score\"].abs() > 2]\n",
    "anomalies"
   ]
  },
