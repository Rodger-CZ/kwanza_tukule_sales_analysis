    "from statsmodels.tsa.holtwinters import ExponentialSmoothing\n",
    "\n",
    "model = ExponentialSmoothing(\n",
    "    ts[\"unit_price\"],\n",
    "    trend=\"add\",\n",
    "    seasonal=None\n",
    ")\n",
    "\n",
    "fit = model.fit()\n",
    "forecast = fit.forecast(3)\n",
    "\n",
    "forecast"
   ]
